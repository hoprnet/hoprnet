{
  "name": "@hoprnet/hopr-protos",
  "version": "1.17.0-alpha.43",
  "files": [
    "node/",
    "web/",
    "protos/",
    "docs/",
    "README.md"
  ],
  "main": "index.js",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "yarn build:protos",
    "build:protos": "yarn build:protos:node && yarn build:protos:web",
    "build:protos:node": "rimraf node && mkdirp node && node ./scripts/generateNode.js",
    "build:protos:web": "rimraf web && mkdirp web && node ./scripts/generateWeb.js",
    "fix:types": "replace 'from \"grpc\"' 'from \"@grpc/grpc-js\"' . -r --include=\"*.d.ts\"",
    "test": "yarn build:protos",
    "prepublishOnly": "yarn build"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://hoprnet.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/hoprnet/hopr-core.git"
  },
  "engines": {
    "node": ">=12.9.1",
    "yarn": "1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "dependencies": {
    "google-protobuf": "^3.12.2",
    "grpc": "^1.24.3"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.7.2",
    "@types/node": "^12.11.0",
    "glob": "^7.1.6",
    "grpc-tools": "^1.9.0",
    "microbundle": "^0.12.2",
    "mkdirp": "^1.0.4",
    "protoc-gen-grpc-web": "^1.2.0",
    "replace": "^1.2.0",
    "rimraf": "^3.0.2",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "grpc-web": "^1.2.0"
  }
}
