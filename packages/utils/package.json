    "jsonschema": "1.4.0",
    "leveldown": "6.1.0",
    "levelup": "5.1.1",
    "libp2p-crypto": "0.21.0",
    "memdown": "6.1.1",
    "multiaddr": "10.0.1",
    "multiformats": "9.5.8",
