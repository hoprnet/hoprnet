    "rewiremock": "3.14.3",
    "rimraf": "3.0.2",
    "sinon": "12.0.1",
    "ts-node": "10.4.0",
    "typedoc": "0.22.11",
    "typedoc-plugin-markdown": "3.11.12",
    "typescript": "4.5.5"
